{
  "name": "my-turborepo-template",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "turbo": "2.5.0",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "npm@11.3.0",
  "volta": {
    "node": "22.14.0",
    "npm": "11.3.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
